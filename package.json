{
  "name": "@keyword-finder/backend",
  "version": "1.0.0",
  "description": "네이버 검색 키워드 수집기 백엔드 - 자동완성, 함께 많이 찾는, 인기주제 데이터 수집",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "nodemon --exec ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "migrate": "npm run migration:run",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "db:reset": "ts-node scripts/reset-database.ts",
    "db:reset-migrations": "ts-node scripts/reset-migrations.ts",
    "db:clear-data": "ts-node scripts/clear-data.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "docker:build": "docker build -t keyword-finder-backend .",
    "docker:run": "docker run -p 3000:3000 keyword-finder-backend",
    "vercel:deploy": "vercel --prod",
    "vercel:dev": "vercel dev"
  },
  "keywords": [
    "naver",
    "keyword",
    "scraping",
    "playwright",
    "n8n"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/pg": "^8.15.5",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.17",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
